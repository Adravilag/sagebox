{
  "name": "@saged-ui/components",
  "version": "1.0.0",
  "description": "SagedUI Components - Modern Web Components Library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/saged-ui-components/saged-ui-components.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*"
    },
    "./styles/*": {
      "import": "./dist/styles/*"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Adravilag/saged-ui.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/Adravilag/saged-ui/issues"
  },
  "homepage": "https://github.com/Adravilag/saged-ui/tree/main/packages/components#readme",
  "files": [
    "dist/",
    "loader/"
  ],
  "sideEffects": [
    "dist/saged-ui-components/saged-ui-components.esm.js",
    "loader/index.js"
  ],
  "scripts": {
    "build": "stencil build",
    "build:watch": "stencil build --watch",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "test:coverage": "stencil test --spec --coverage"
  },
  "peerDependencies": {
    "@saged-ui/icons": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@saged-ui/icons": {
      "optional": true
    }
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "saged-ui",
    "components",
    "web-components",
    "stencil",
    "design-system",
    "ui-library",
    "custom-elements"
  ],
  "author": "adravilag",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
